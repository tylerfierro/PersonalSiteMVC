                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    @Html.ActionLink("My Personal Site", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
                </div>
                @{
                    var currentController = ViewContext.RouteData.Values["controller"] ?? "Home";
                    var currentAction = ViewContext.RouteData.Values["action"] ?? "Index";
                    var currentRoute = (currentController + "-" + currentAction).ToLower();
                }
                <nav>
                    <ul>
                        <li>
                            <div class="btn btn-primary">
                                @*<li class="start selected"><a href="index.html">Home</a>*@
                                @Html.ActionLink("Home", "Index", "Home", null, new { @class = currentRoute == "home-index" ? "active" : "" })
                            </div>
                        </li>
                        <li>
                            <div class="btn btn-primary">
                                @*<li class="start selected"><a href="Resume.html">Resume</a>*@
                                @Html.ActionLink("Resume", "Resume", "Home", null, new { @class = currentRoute == "home-resume" ? "active" : "" })
                            </div>
                        </li>
                        <li>
                            <div class="btn btn-primary">
                                @*<li class="start selected"><a href="Links.html">Links</a>*@
                                @Html.ActionLink("Links", "Links", "Home", null, new { @class = currentRoute == "home-links" ? "active" : "" })
                            </div>
                        </li>
                        <li>
                            <div class="btn btn-primary">
                                @*<li class="start selected"><a href="Projects.html">Projects</a>*@
                                @Html.ActionLink("Projects", "Projects", "Home", null, new { @class = currentRoute == "home-projects" ? "active" : "" })
                            </div>
                        </li>
                        <li>
                            <div class="btn btn-primary">
                                @*<li class="start selected"><a href="Contact.html">Contact</a>*@
                                @Html.ActionLink("Contact", "Contact", "Home", null, new { @class = currentRoute == "home-contact" ? "active" : "" })
                            </div>
                        </li>
                    </ul>
                    @*@Html.Partial("_LoginPartial")*@
                </nav>

